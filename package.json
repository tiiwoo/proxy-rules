{
  "name": "ruleset.tiiwoo.moe",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build": "wireit",
    "build:apple-cdn": "wireit",
    "build:apple-tpl": "wireit",
    "build:chn-cidr": "wireit",
    "build:stream": "wireit",
    "build:public": "wireit",
    "validate:domainset": "wireit"
  },
  "wireit": {
    "build:apple-cdn": {
      "command": "node ./Build/build-apple-cdn.js"
    },
    "build:apple-tpl": {
      "command": "node ./Build/surgio/build-apple-tpl.js"
    },
    "build:chn-cidr": {
      "command": "node ./Build/build-chn-cidr.js"
    },
    "build:stream": {
      "command": "node ./Build/build-stream.js"
    },
    "build:public": {
      "command": "node ./Build/build-public.js",
      "dependencies": [
        "build:apple-cdn",
        "build:apple-tpl",
        "build:chn-cidr",
        "build:stream"
      ]
    },
    "validate:domainset": {
      "command": "node ./Build/validate-domainset.js",
      "dependencies": [
        "build:apple-cdn",
        "build:apple-tpl",
        "build:chn-cidr",
        "build:stream"
      ]
    },
    "build": {
      "dependencies": [
        "build:apple-cdn",
        "build:apple-tpl",
        "build:chn-cidr",
        "build:stream",
        "build:public",
        "validate:domainset"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tiiwoo/Surge.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sukka/listdir": "^0.2.0",
    "@vercel/fetch-retry": "^5.1.3",
    "ci-info": "^3.8.0",
    "cidr-tools": "^5.1.5",
    "fs-extra": "^11.1.1",
    "node-fetch": "^2.6.11",
    "picocolors": "^1.0.0",
    "piscina": "^3.2.0",
    "table": "^6.8.1",
    "tldts": "^6.0.5",
    "undici": "5.22.1"
  },
  "devDependencies": {
    "wireit": "^0.9.5"
  },
  "engines": {
    "node": "> 18.0.0"
  }
}
